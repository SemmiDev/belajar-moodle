{
  "title": "Panduan Lengkap Moodle dengan Docker",
  "description": "Buku panduan komprehensif untuk mempelajari Moodle LMS dengan Docker Compose",
  "author": "Moodle Learning Team",
  "language": "id",
  "gitbook": ">=3.0.0",
  "root": ".",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "plugins": [
    "expandable-chapters",
    "copy-code-button",
    "page-toc",
    "back-to-top-button",
    "search-pro",
    "chapter-fold"
  ],
  "pluginsConfig": {
    "page-toc": {
      "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3",
      "position": "before-first",
      "showByDefault": true
    }
  },
  "variables": {
    "version": "1.0.0"
  },
  "pdf": {
    "fontSize": 12,
    "footerTemplate": "<div style=\"font-size: 10px;\">Halaman <span class=\"pageNumber\"></span> dari <span class=\"totalPages\"></span></div>",
    "headerTemplate": "<div style=\"font-size: 10px;\">Panduan Moodle dengan Docker</div>"
  }
}